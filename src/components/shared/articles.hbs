<div class="card mb-4">
  <div class="card-body">
    <h4 class="card-title mb-3">{{text.name}}</h4>
    <h6
      class="card-subtitle mb-2 {{text.display.class}}"
    >{{text.display.name}}</h6>
    <p class="lead">{{text.keyWords}}</p>
    <p class="lead">{{text.note}}</p>
    <hr />
    <div class="d-flex justify-content-between">
      <a class="mb-0" href="{{text.link}}" rel="noopener" target="_blank">Read
        in new tab</a>
      <span>Release Date: <strong>{{text.releaseDate}}</strong></span>
    </div>
  </div>
</div>